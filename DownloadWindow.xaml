<Window x:Class="PeakyPaint.DownloadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="New Window" Height="450" Width="800">
    <Grid>
        <!-- Define rows and columns for the grid -->
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <!-- 9 Buttons (3x3 Grid) -->
        <Button Grid.Row="0" Grid.Column="0" Name="Button1" Click="Button1_Click" />
        <Button Grid.Row="0" Grid.Column="1" Name="Button2" Click="Button2_Click" />
        <Button Grid.Row="0" Grid.Column="2" Name="Button3" Click="Button3_Click" />
        <Button Grid.Row="1" Grid.Column="0" Name="Button4" Click="Button4_Click" />
        <Button Grid.Row="1" Grid.Column="1" Name="Button5" Click="Button5_Click" />
        <Button Grid.Row="1" Grid.Column="2" Name="Button6" Click="Button6_Click" />
        <Button Grid.Row="2" Grid.Column="0" Name="Button7" Click="Button7_Click"  />
        <Button Grid.Row="2" Grid.Column="1" Name="Button8" Click="Button8_Click" />
        <Button Grid.Row="2" Grid.Column="2" Content="RETURN" FontSize= "50" Background="Aqua" Name="ReturnButton" Click="ReturnButton_Click" />

       
    </Grid>
</Window>